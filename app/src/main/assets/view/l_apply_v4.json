{
  "cname": "RelativeLayout",
  "android:id": "@+id/root_vg",
  "android:layout_width": "match_parent",
  "android:layout_height": "wrap_content",
  "android:paddingBottom": "8dp",
  "android:paddingLeft": "16dp",
  "android:paddingRight": "16dp",
  "android:paddingTop": "8dp",
  "android:tag": "root_vg",
  "childs": [
    {
      "cname": "ImageView",
      "android:id": "@+id/icon_iv",
      "android:layout_width": "40dp",
      "android:layout_height": "40dp",
      "android:src": "@mipmap/apply",
      "android:tag": "icon_iv"
    },
    {
      "cname": "TextView",
      "android:id": "@+id/tv0",
      "android:layout_width": "wrap_content",
      "android:layout_height": "wrap_content",
      "android:layout_marginLeft": "8dp",
      "android:layout_marginStart": "8dp",
      "android:layout_toEndOf": "@+id/icon_iv",
      "android:layout_toRightOf": "@+id/icon_iv",
      "android:ellipsize": "end",
      "android:maxLines": "1",
      "android:tag": "tv0",
      "android:textColor": "#000000",
      "android:textSize": "12sp",
      "android:text": "流水号 FG-2018-01-22"
    },
    {
      "cname": "TextView",
      "android:id": "@+id/tv1",
      "android:layout_width": "wrap_content",
      "android:layout_height": "wrap_content",
      "android:layout_below": "@+id/tv0",
      "android:layout_marginLeft": "8dp",
      "android:layout_marginStart": "8dp",
      "android:layout_marginTop": "4dp",
      "android:layout_toEndOf": "@+id/icon_iv",
      "android:layout_toRightOf": "@+id/icon_iv",
      "android:ellipsize": "end",
      "android:maxLines": "1",
      "android:tag": "tv1",
      "android:textColor": "#000000",
      "android:textSize": "12sp",
      "android:text": "张三"
    },
    {
      "cname": "TextView",
      "android:id": "@+id/tv2",
      "android:layout_width": "wrap_content",
      "android:layout_height": "wrap_content",
      "android:layout_below": "@+id/tv0",
      "android:layout_centerHorizontal": "true",
      "android:layout_marginTop": "4dp",
      "android:ellipsize": "end",
      "android:maxLines": "1",
      "android:paddingLeft": "20dp",
      "android:paddingRight": "20dp",
      "android:tag": "tv2",
      "android:textColor": "#000000",
      "android:textSize": "12sp",
      "android:text": "信息技术部"
    },
    {
      "cname": "TextView",
      "android:id": "@+id/tv3",
      "android:layout_width": "wrap_content",
      "android:layout_height": "wrap_content",
      "android:layout_alignParentEnd": "true",
      "android:layout_alignParentRight": "true",
      "android:layout_below": "@+id/tv0",
      "android:layout_marginTop": "4dp",
      "android:ellipsize": "end",
      "android:maxLines": "1",
      "android:tag": "tv3",
      "android:textColor": "#000000",
      "android:textSize": "12sp",
      "android:text": "2018-02-21 发起"
    },
    {
      "cname": "LinearLayout",
      "android:id": "@+id/vg0",
      "android:layout_width": "match_parent",
      "android:layout_height": "wrap_content",
      "android:layout_below": "@+id/icon_iv",
      "android:layout_marginTop": "4dp",
      "android:orientation": "horizontal",
      "childs": [
        {
          "cname": "TextView",
          "android:id": "@+id/tv4",
          "android:layout_width": "96dp",
          "android:layout_height": "wrap_content",
          "android:background": "@drawable/l_kuang_shape",
          "android:ellipsize": "end",
          "android:gravity": "center",
          "android:maxLines": "1",
          "android:tag": "tv4",
          "android:textColor": "#68ABFA",
          "android:textSize": "12sp",
          "android:text": "广告小礼品"
        },
        {
          "cname": "TextView",
          "android:id": "@+id/tv5",
          "android:layout_width": "0dp",
          "android:layout_height": "wrap_content",
          "android:layout_weight": "1.4",
          "android:gravity": "right",
          "android:maxLines": "1",
          "android:paddingLeft": "8dp",
          "android:tag": "tv5",
          "android:textColor": "#000000",
          "android:textSize": "12sp",
          "android:text": "￥10000.00"
        },
        {
          "cname": "TextView",
          "android:id": "@+id/tv6",
          "android:layout_width": "0dp",
          "android:layout_height": "wrap_content",
          "android:layout_marginLeft": "16dp",
          "android:layout_marginStart": "16dp",
          "android:layout_weight": "1",
          "android:ellipsize": "end",
          "android:maxLines": "1",
          "android:tag": "tv6",
          "android:textColor": "#000000",
          "android:textSize": "12sp",
          "android:text": "10000件"
        },
        {
          "cname": "TextView",
          "android:id": "@+id/tv7",
          "android:layout_width": "0dp",
          "android:layout_height": "wrap_content",
          "android:layout_weight": "2",
          "android:gravity": "right",
          "android:maxLines": "1",
          "android:paddingLeft": "8dp",
          "android:tag": "tv7",
          "android:textColor": "#000000",
          "android:textSize": "12sp",
          "android:text": "2018-02-21 入库"
        }
      ]
    },
    {
      "cname": "TextView",
      "android:id": "@+id/tv8",
      "android:layout_width": "wrap_content",
      "android:layout_height": "wrap_content",
      "android:layout_below": "@+id/vg0",
      "android:layout_marginTop": "4dp",
      "android:ellipsize": "end",
      "android:maxLines": "1",
      "android:tag": "tv8",
      "android:text": "用途",
      "android:textColor": "#000000",
      "android:textSize": "12sp"
    },
    {
      "cname": "TextView",
      "android:id": "@+id/tv9",
      "android:layout_width": "wrap_content",
      "android:layout_height": "wrap_content",
      "android:layout_below": "@+id/vg0",
      "android:layout_marginLeft": "20dp",
      "android:layout_marginStart": "20dp",
      "android:layout_marginTop": "4dp",
      "android:layout_toEndOf": "@+id/tv8",
      "android:layout_toRightOf": "@+id/tv8",
      "android:ellipsize": "end",
      "android:maxLines": "1",
      "android:tag": "tv9",
      "android:textColor": "#FF8040",
      "android:textSize": "12sp",
      "android:text": "员工福利"
    }
  ]
}