{
  "name": "RelativeLayout",
  "android:id": "@+id/root_vg",
  "android:layout_width": "match_parent",
  "android:layout_height": "wrap_content",
  "android:paddingBottom": "8dp",
  "android:paddingLeft": "16dp",
  "android:paddingRight": "16dp",
  "android:paddingTop": "8dp",
  "android:tag": "root_vg",
  "childs": [
    {
      "name": "ImageView",
      "android:id": "@+id/icon_iv",
      "android:layout_width": "40dp",
      "android:layout_height": "40dp",
      "android:src": "@mipmap/apply",
      "android:tag": "icon_iv"
    },
    {
      "name": "LinearLayout",
      "android:id": "@+id/vg",
      "android:layout_width": "match_parent",
      "android:layout_height": "wrap_content",
      "android:layout_marginLeft": "8dp",
      "android:layout_toEndOf": "@+id/icon_iv",
      "android:layout_toRightOf": "@+id/icon_iv",
      "android:orientation": "horizontal",
      "childs": [
        {
          "name": "TextView",
          "android:id": "@+id/tv0",
          "android:layout_width": "wrap_content",
          "android:layout_height": "wrap_content",
          "android:ellipsize": "end",
          "android:maxLines": "1",
          "android:tag": "tv0",
          "android:text": "编号",
          "android:textColor": "#000000",
          "android:textSize": "12sp"
        },
        {
          "name": "TextView",
          "android:id": "@+id/tv1",
          "android:layout_width": "0dp",
          "android:layout_height": "wrap_content",
          "android:layout_weight": "4",
          "android:ellipsize": "start",
          "android:gravity": "right",
          "android:maxLines": "1",
          "android:paddingLeft": "20dp",
          "android:tag": "tv1",
          "android:textColor": "#808000",
          "android:textSize": "12sp",
          "android:text": "FGMC-ZD-20180302-0001"
        },
        {
          "name": "TextView",
          "android:id": "@+id/tv2",
          "android:layout_width": "wrap_content",
          "android:layout_height": "wrap_content",
          "android:layout_marginLeft": "40dp",
          "android:layout_marginStart": "40dp",
          "android:ellipsize": "end",
          "android:maxLines": "1",
          "android:tag": "tv2",
          "android:text": "金额",
          "android:textColor": "#000000",
          "android:textSize": "12sp"
        },
        {
          "name": "TextView",
          "android:id": "@+id/tv3",
          "android:layout_width": "0dp",
          "android:layout_height": "wrap_content",
          "android:layout_weight": "1.5",
          "android:gravity": "right",
          "android:maxLines": "1",
          "android:paddingLeft": "20dp",
          "android:tag": "tv3",
          "android:textColor": "#808000",
          "android:textSize": "12sp",
          "android:text": "100000.00"
        }
      ]
    },
    {
      "name": "TextView",
      "android:id": "@+id/tv4",
      "android:layout_width": "wrap_content",
      "android:layout_height": "wrap_content",
      "android:layout_alignLeft": "@+id/vg",
      "android:layout_below": "@+id/vg",
      "android:layout_marginTop": "4dp",
      "android:ellipsize": "end",
      "android:maxLines": "1",
      "android:tag": "tv4",
      "android:textColor": "#000000",
      "android:textSize": "12sp",
      "android:text": "李强"
    },
    {
      "name": "TextView",
      "android:id": "@+id/tv5",
      "android:layout_width": "wrap_content",
      "android:layout_height": "wrap_content",
      "android:layout_below": "@+id/vg",
      "android:layout_centerHorizontal": "true",
      "android:layout_marginTop": "4dp",
      "android:ellipsize": "end",
      "android:maxLines": "1",
      "android:paddingLeft": "20dp",
      "android:tag": "tv5",
      "android:textColor": "#000000",
      "android:textSize": "12sp",
      "android:text": "部门一"
    },
    {
      "name": "TextView",
      "android:id": "@+id/tv6",
      "android:layout_width": "wrap_content",
      "android:layout_height": "wrap_content",
      "android:layout_alignRight": "@+id/vg",
      "android:layout_below": "@+id/vg",
      "android:layout_marginTop": "4dp",
      "android:ellipsize": "end",
      "android:maxLines": "1",
      "android:paddingLeft": "20dp",
      "android:tag": "tv6",
      "android:textColor": "#000000",
      "android:textSize": "12sp",
      "android:text": "2018-06-10"
    },
    {
      "name": "TextView",
      "android:id": "@+id/tv7",
      "android:layout_width": "wrap_content",
      "android:layout_height": "wrap_content",
      "android:layout_below": "@+id/icon_iv",
      "android:layout_marginTop": "4dp",
      "android:ellipsize": "end",
      "android:maxLines": "1",
      "android:tag": "tv7",
      "android:text": "招待事由",
      "android:textColor": "#000000",
      "android:textSize": "12sp"
    },
    {
      "name": "TextView",
      "android:id": "@+id/tv8",
      "android:layout_width": "wrap_content",
      "android:layout_height": "wrap_content",
      "android:layout_below": "@+id/icon_iv",
      "android:layout_marginLeft": "20dp",
      "android:layout_marginTop": "4dp",
      "android:layout_toRightOf": "@+id/tv7",
      "android:ellipsize": "end",
      "android:maxLines": "1",
      "android:tag": "tv8",
      "android:textColor": "#808000",
      "android:textSize": "12sp",
      "android:text": "招待事由"
    }
  ]
}